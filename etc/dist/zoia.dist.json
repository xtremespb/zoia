{
    "webServer": {
        "ip": "127.0.0.1",
        "port": 3001,
        "user": "",
        "group": ""
    },
    "serveStatic": true,
    "siteOptions": {
        "server": "localhost",
        "id": "z3"
    },
    "pm2": {
        "script": "./build/bin/zoia.js",
        "watch": false,
        "exec_mode": "cluster",
        "instances": 0,
        "error_file": true,
        "out_file": true,
        "log_file": false,
        "merge_logs": true,
        "time": false
    },
    "secret": "",
    "token": {
        "expires": "7 days",
        "ip": true
    },
    "logLevel": "info",
    "trustProxy": true,
    "stackTrace": true,
    "mailer": {
        "service": "Yandex",
        "auth": {
            "user": "info@zoiajs.org",
            "pass": ""
        }
    },
    "mongo": {
        "url": "mongodb://127.0.0.1:27017",
        "dbName": "zoia3"
    },
    "redis": {
        "enabled": false,
        "host": "127.0.0.1",
        "port": 6379
    },
    "rateLimit": {
        "enabled": false,
        "redis": false,
        "ban": false,
        "global": false,
        "whiteList": [],
        "blackList": [],
        "addHeaders": {
            "x-ratelimit-limit": true,
            "x-ratelimit-remaining": true,
            "x-ratelimit-reset": true,
            "retry-after": true
        }
    },
    "cookieOptions": {
        "expires": 604800,
        "path": "/",
        "domain": "",
        "secure": null,
        "sameSite": null
    },
    "captchaValidity": 3600,
    "originCORS": "*",
    "languages": {
        "en": "English",
        "ru": "Русский"
    },
    "siteMetadata": {
        "en": {
            "title": "Zoia Website",
            "titleShort": "Zoia Website"
        },
        "ru": {
            "title": "Сайт на Zoia",
            "titleShort": "Сайт на Zoia"
        }
    },
    "commonTableItemsLimit": 20,
    "collections": {
        "files": "files",
        "registry": "registry"
    },
    "directories": {
        "files": "build/files"
    },
    "routes": {
        "download": "/zoia/core/download",
        "login": "/users/login"
    },
    "telegram": {
        "enabled": false,
        "token": ""
    }
}
