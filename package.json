{
    "name": "zoia2",
    "version": "2.0.6x",
    "description": "Zoia CMS",
    "main": "dist/bin/api.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/xtremespb/zoia2"
    },
    "scripts": {
        "start-dev": "node ./scripts/devStart.js",
        "build": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js",
        "release": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --do:release",
        "build-dev": "webpack --display minimal --progress --mode development --config ./src/shared/build/webpack.config.js",
        "build-admin": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --build:admin",
        "build-admin-dev": "webpack --display minimal --progress --mode development --config ./src/shared/build/webpack.config.js --build:admin",
        "build-web": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --build:web",
        "build-web-dev": "webpack --display minimal --progress --mode development --config ./src/shared/build/webpack.config.js --build:web",
        "build-api": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --build:api",
        "build-api-dev": "webpack --display minimal --progress --mode development --config ./src/shared/build/webpack.config.js --build:api",
        "build-tools": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --build:tools",
        "build-tools-dev": "webpack --display minimal --progress --mode development --config ./src/shared/build/webpack.config.js --build:tools",
        "update": "webpack --display minimal --progress --mode production --config ./src/shared/build/webpack.config.js --build:update",
        "add-locale": "lingui add-locale --config ./src/shared/build/linguirc.admin.json",
        "extract": "lingui extract --config ./src/shared/build/linguirc.admin.json",
        "compile": "lingui compile --config ./src/shared/build/linguirc.admin.json",
        "configure": "node ./scripts/configure.js",
        "server-config": "node ./scripts/serverConfig.js",
        "install-modules": "node ./dist/bin/tools.js -i",
        "install-defaults": "node ./dist/bin/tools.js -r",
        "split-locales": "node ./dist/bin/tools.js -s",
        "combine-locales": "node ./dist/bin/tools.js -c"
    },
    "keywords": [
        "zoia2",
        "cms",
        "react",
        "uikit",
        "mongo",
        "fastify"
    ],
    "author": "Michael Matveev <xtreme@rh1.ru>",
    "license": "MIT",
