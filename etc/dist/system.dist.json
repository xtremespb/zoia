{
    "id": "zoia3",
    "server": "localhost",
    "url": "http://127.0.0.1:3001",
    "webServer": {
        "ip": "127.0.0.1",
        "port": 3001,
        "user": "",
        "group": ""
    },
    "serveStatic": true,
    "pm2": {
        "enabled": false,
        "script": "./build/bin/zoia.js",
        "watch": false,
        "exec_mode": "cluster",
        "instances": 0,
        "error_file": true,
        "out_file": true,
        "log_file": false,
        "merge_logs": true,
        "time": false
    },
    "secret": "",
    "token": {
        "expires": "7 days",
        "ip": false
    },
    "logLevel": "info",
    "trustProxy": true,
    "stackTrace": true,
    "modules": ["backup", "files", "nav", "pages", "registry", "update"],
    "templates": ["default"],
    "email": {
        "template": "default",
        "mailer": {
            "service": "Yandex",
            "auth": {
                "user": "",
                "pass": ""
            }
        },
        "logoFile": "z3_mail_logo.png",
        "from": "ZOIA <info@zoiajs.org>"
    },
    "mongo": {
        "url": "mongodb://127.0.0.1:27017",
        "dbName": "zoia3"
    },
    "redis": {
        "enabled": false,
        "host": "127.0.0.1",
        "port": 6379
    },
    "socketIO": {
        "path": "/socket.io",
        "serveClient": true
    },
    "rateLimit": {
        "enabled": false,
        "redis": false,
        "ban": false,
        "global": false,
        "whiteList": [],
        "blackList": [],
        "addHeaders": {
            "x-ratelimit-limit": true,
            "x-ratelimit-remaining": true,
            "x-ratelimit-reset": true,
            "retry-after": true
        }
    },
    "cookieOptions": {
        "expires": 604800,
        "path": "/",
        "domain": "",
        "secure": null,
        "sameSite": null
    },
    "captchaValidity": 3600,
    "originCORS": "*",

    "telegram": {
        "enabled": false,
        "token": ""
    },
    "test": {
        "headless": true,
        "args": ["--window-size=1920,1080"],
        "defaultViewport": null
    }
}
