{
    "collectionUsers": "users",
    "collectionAcl": "acl",
    "database": {
        "collections": {
            "users": {
                "indexesAsc": ["username", "displayName", "email"],
                "indexesDesc": ["username", "displayName", "email"],
                "expires": 259200,
                "expireField": "deletedAt"
            },
            "acl": {
                "indexesAsc": ["group"],
                "indexesDesc": ["group"]
            }
        }
    },
    "routes": {
        "users": "/admin/users",
        "acl": "/admin/acl",
        "login": "/users/login",
        "logout": "/users/logout"
    },
    "setup": false
}
