<router routes=[
  {
    name: "yachts",
    path: out.global.routes.admin
  },
  {
    name: "yachts.edit",
    path: "/edit/:id"
  }
] on-state-change("onStateChange")>
  <if(state.route.name === "yachts")>
    <yachtsList ...state.route.params/>
  </if>
  <if(state.route.name === "yachts.edit")>
    <yachtEdit ...state.route.params/>
  </if>
</router>
