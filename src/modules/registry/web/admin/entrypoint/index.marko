<router routes=[
  {
    name: "data",
    path: out.global.routes.registry
  },
  {
    name: "data.edit",
    path: "/edit/:id"
  }
] on-state-change("onStateChange")>
  <if(state.route.name === "data")>
    <dataList ...state.route.params/>
  </if>
  <if(state.route.name === "data.edit")>
    <dataEdit ...state.route.params/>
  </if>
</router>
